@*<html xmlns="http://www.w3.org/1999/xhtml">*@
<head>
    <title>@ViewData["Title"] - AspNetNewsSite</title>
    @*<title>News Magazine</title>*@
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/styles/layout.css" type="text/css">
    <script type="text/javascript" src="~/scripts/jquery.min.js"></script>
    <!-- Homepage Specific -->
    <script type="text/javascript" src="~/scripts/galleryviewthemes/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="~/scripts/galleryviewthemes/jquery.timers.1.2.js"></script>
    <script type="text/javascript" src="~/scripts/galleryviewthemes/jquery.galleryview.2.1.1.min.js"></script>
    <script type="text/javascript" src="~/scripts/galleryviewthemes/jquery.galleryview.setup.js"></script>
    <!-- / Homepage Specific -->
</head>
<body id="top">
    <div class="panel" style="width: 600px; height: 280px; position: absolute; overflow: hidden; top: 15px; left: 15px; display: block; opacity: 0.976361;">
        <img src="images/demo/1.gif" style="height: 280px; width: 600px; position: relative; top: 0px; left: 0px;"><div class="panel-overlay" style="position: absolute; z-index: 999; width: 580px; left: 0px; bottom: 0px;">
            <h2>Nullamlacus dui ipsum</h2>
            <p>
                Temperinte interdum sempus odio urna eget curabitur semper convallis nunc laoreet.<br>
                <a href="#">Continue Reading »</a>
            </p>
        </div>
        <div class="overlay-background" style="position: absolute; z-index: 998; width: 600px; left: 0px; opacity: 0.7; bottom: 0px;"></div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!-- ####################################################################################################### -->

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
